program divisor {
  number i, n, d.
  d = 0.
  read n.
  if n > 0 {
    i = 2.
    loop i <= n/2 {
      if n % i == 0 {
	write n, " e' divisivel por ", i, "\n".
	d = d + 1.
      }.
      i = i + 1.
    }.
    if d == 0 {
      write n, " e' primo", "\n".
    }.
  }.
  return 0.
}
