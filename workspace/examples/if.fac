public void prints(string s)
public void printi(string s)
public integer readi()
integer x
integer b
integer c

public integer entry (integer argc, string *argv, string *envp) {
  x := readi()
  b := -4
  c :=  b

  printi(x)
  printi(b)
  printi(c)

  if x > 0 then
    prints("positivo!\n")
  if x < 0 then
    prints("negativo!\n")
  if x = 0 then
    prints("nulo!\n")
  entry := 0
};
